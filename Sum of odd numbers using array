#include <iostream>
using namespace std;

int main()
{
    int n,i;
    int sum = 0;
    cout << "Enter the number of elements: ";
    cin >> n;
    int arr[n];  

    cout << "Enter the elements:\n";
    for (i = 0; i < n; i++) {
        cin >> arr[i]; 
    }
      for (i = 0; i < n; i++) {
        if (arr[i] % 2 != 0) { 
            sum += arr[i];      
        }
    }

    cout << "Sum of odd numbers = " << sum << endl;

    return 0;
}
