#include<iostream>
using namespace std;
 void sumElements(int arr[],int n){
    int sum= 0;
   for(int i=0;i<n;i++){
   sum=sum+arr[i];
   }
  cout<<"sum of Elements: "<<sum;
}
int main()
{
int i,n;
cout<<"enter size of array: ";
cin>>n;
int arr[n];
cout<<"enter elements: ";
for(i=0; i<n; i++){
   cin>>arr[i];
  }
  sumElements(arr,n);
return 0;
}
