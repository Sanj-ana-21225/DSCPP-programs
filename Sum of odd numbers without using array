#include<iostream>
using namespace std;
int main()
{
  int n;
  cout<<"enter no. of elements: ":
  cin>>n;
  int sum=0,num,i;
  cout<<"enter the elements:\n";
  for(i=0; i<n; i++){
    cin>>num;
   if (num % 2 != 0){
    sum=sum+num;
    }
   }
  cout<<"sum of odd numbers: "<<sum<<endl;

  return 0;
}
